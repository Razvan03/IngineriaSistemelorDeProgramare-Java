package isp.lab3;

import isp.lab3.exercise4.MyPoint;
import org.junit.Test;

import static org.junit.Assert.assertEquals;
/**
 *
 * @author Antonia
 */
public class MyPointTest {
    MyPoint p1 = new MyPoint(1, 1, 1);
    MyPoint p2 = new MyPoint(1, 1, 1);
    
    @Test
    public void testDistanceCoord() {
        assertEquals("Should return the distance", 1,  p1.distance(1, 1, 2));
    }
    
    @Test
    public void testDistancePoint() {
        assertEquals("Should return the distance", 0,  p1.distance(p2));
    }
}
